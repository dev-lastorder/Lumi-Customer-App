{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "paths": {
      "@/*": ["./src/*", "./*"],
      "tailwind.config": ["./tailwind.config.js"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "global.d.ts", "nativewind-env.d.ts", "src/redux/types"]
}
